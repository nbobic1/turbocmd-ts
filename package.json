{
  "name": "turbocmd",
  "publisher": "ReincarnatedShrike",
  "displayName": "TurboCMD",
  "description": "CMD on steroids",
  "version": "0.1.0",
  "icon":"./icon.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:proba"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "proba",
          "title": "TurboCMD",
          "icon": "resources/package-explorer.svg"
        }
      ]
    },
    "views": {
      "proba": [
        {
          "type": "webview",
          "id": "treeview-turbocmd",
          "name": "Tree view",
          "contextualTitle": "Tree view"
        },
        {
          "type": "webview",
          "id": "buttons-turbocmd",
          "name": "Buttons",
          "contextualTitle": "Button"
        },
        {
          "type": "webview",
          "id": "term-turbocmd",
          "name": "Terminal",
          "contextualTitle": "yerm"
        }
      ]
    },
    "commands": [
      {
        "command": "turbocmd.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w --bundleConfigAsCjs\" \"webpack --watch\"",
    "package": "concurrently \"rollup -c --bundleConfigAsCjs\" \"webpack --mode production --devtool hidden-source-map\"",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.4",
    "@tsconfig/svelte": "^5.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "concurrently": "^8.2.1",
    "eslint": "^8.47.0",
    "fs": "^0.0.1-security",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "rollup": "^3.29.4",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
